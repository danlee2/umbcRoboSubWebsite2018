<!-- Page Header -->
<!-- Set your background image for this header in the theme's configuration: index_cover -->
<header class="intro-header" style="background-image: url('<%- theme.index_cover %>')">
    <div class="container">
        <div class="row">
            <div class="col-lg-1 col-lg-offset-2 col-md-10 col-md-offset-1">
                <div class="site-heading" style="font-size:160px;">
                    <h1><%- config.title %></h1>
                    <!-- <hr class="small"> -->
                    <span class="subheading"><%- config.subtitle %></span>
                </div>
            </div>
        </div>
    </div>
</header>
